
.content {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}
.title {
  font-size: 36rpx;
  color: #8f8f94;
}
.box {
  box-shadow: 0 1rpx 10rpx #ececec;
  background-color: #fff;
  margin-bottom: 30rpx;
  border-radius: 10rpx;
  position: relative;
}
.box .h2 {
  font-weight: 400;
  color: #999999;
  font-size: 26rpx;
  padding: 15rpx 20rpx 10rpx;
  border-bottom: 1px dotted #e4e4e4;
}
.box .h2 .a {
  color: #f68e8e;
  font-size: 28.000000000000004rpx;
  float: right;
}
.box .ul {
  padding: 20rpx;
}
.box .ul .li {
  margin-bottom: 0.2em;
}